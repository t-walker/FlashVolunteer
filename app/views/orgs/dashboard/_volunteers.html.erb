      test text test texttest texttest texttest texttest texttest texttest texttest text
      test texttest texttest texttest texttest texttest texttest texttest texttest test text
      test text test texttest texttest texttest texttest text test texttest texttest texttest text
    </div>
    <div class="span3 pull-right">
        <div class="row">
          <div class="input-append bootstrap-timepicker pull-right">
              <input id="startdate" type="text" class="input-full" value="<%= Time.now.to_date %>"/>
              <span class="add-on">
                  <i class="icon-time"></i>
              </span>
          </div>
        </div>
        <div class="row">
          <div class="input-append bootstrap-timepicker pull-right">
              <input id="enddate" type="text" class="input-full" value="<%= Time.now.to_date + 2.month%>"/>
              <span class="add-on">
                  <i class="icon-time"></i>
              </span>
          </div>
        </div>
    </div>
    <script type="text/javascript">
        $(document).ready(function() { $('#startdate').datepicker({dateFormat: 'm/d/yy'})});
        $(document).ready(function() { $('#enddate').datepicker({dateFormat: 'm/d/yy'})});

    </script>
  </div>
</div>

<div class="row-fluid">
  <div class="span12">
  <!-- Your Volunteers -->
      <h3>Total Volunteers</h3>
         <script type="text/javascript"
               src="https://www.google.com/jsapi?autoload={
                 'modules':[{
                   'name':'visualization',
                   'version':'1',
                   'packages':['corechart']
                 }]
               }"></script>

         <script type="text/javascript">
           google.setOnLoadCallback(drawChart);

           function drawChart() {
             var data = google.visualization.arrayToDataTable([
               ['Year', 'V'],
               ['2004',  1000],
               ['2005',  1170],
               ['2006',  660],
               ['2007',  1030]
             ]);

             var options = {
               title: 'Total Volunteers',
               curveType: 'none',
               legend: { position: 'bottom' }
             };

             var chart = new google.visualization.LineChart(document.getElementById('total_vol'));

             chart.draw(data, options);
           }
         </script>
       </head>
         <div id="total_vol" ></div>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
  <!-- Your Volunteers -->
      <h3>New Volunteers</h3>
      <script type="text/javascript"
            src="https://www.google.com/jsapi?autoload={
              'modules':[{
                'name':'visualization',
                'version':'1',
                'packages':['corechart']
              }]
            }"></script>

      <script type="text/javascript">
        google.setOnLoadCallback(drawChart);

        function drawChart() {
          var data = google.visualization.arrayToDataTable([
            ['Year', 'Volunteers'],
            ['2004',  20],
            ['2005',  15],
            ['2006',  5],
            ['2007',  50],
            ['2008', 20]
          ]);

          var options = {
            title: 'New Volunteers',
            curveType: 'none',
            legend: { position: 'bottom' }
          };

          var chart = new google.visualization.LineChart(document.getElementById('new_vol'));

          chart.draw(data, options);
        }
      </script>
    </head>
      <div id="new_vol" ></div>  </div>
</div>

