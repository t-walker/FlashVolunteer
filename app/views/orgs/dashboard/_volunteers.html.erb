<div class="row-fluid">
  <div class="span12">
  <!-- Grabbers -->
    <div class="span8 pull-left">
        test
    </div>
    <div class="span4 pull-right">
        <div class="row">
          <div class="input-append bootstrap-timepicker pull-right">
              <input id="startdate" type="text" class="input-full" value="<%= Time.now.to_date %>"/>
              <span class="add-on">
                  <i class="icon-time"></i>
              </span>
          </div>
        </div>
        <div class="row">
          <div class="input-append bootstrap-timepicker pull-right">
              <input id="enddate" type="text" class="input-full" value="<%= Time.now.to_date + 2.month%>"/>
              <span class="add-on">
                  <i class="icon-time"></i>
              </span>
          </div>
        </div>
    </div>
    <script type="text/javascript">
        $(document).ready(function() { $('#startdate').datepicker({dateFormat: 'm/d/yy'})});
        $(document).ready(function() { $('#enddate').datepicker({dateFormat: 'm/d/yy'})});

    </script>
  </div>
</div>

